Default
	Skill = 50
	Aggression = 50
	ReactionTime = 0.5
	AttackDelay = 0.25
	Teamwork = 75
	WeaponPreference = none
	Cost = 0
	Difficulty = NORMAL
	VoicePitch = 100
	Skin = 0
End

